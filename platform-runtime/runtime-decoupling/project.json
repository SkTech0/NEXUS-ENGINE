{
  "name": "runtime-decoupling",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "platform-runtime/runtime-decoupling",
  "tags": ["scope:platform", "domain:runtime"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/runtime-decoupling",
        "main": "platform-runtime/runtime-decoupling/index.ts",
        "tsConfig": "platform-runtime/runtime-decoupling/tsconfig.json"
      },
      "configurations": {
        "production": {}
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["platform-runtime/runtime-decoupling/**/*.ts"]
      }
    },
    "serve-engine-core": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/runtime-decoupling/runners/engine-core-runner.js",
        "dependsOn": ["build"]
      }
    },
    "serve-engine-ai": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/runtime-decoupling/runners/engine-ai-runner.js",
        "dependsOn": ["build"]
      }
    },
    "serve-engine-data": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/runtime-decoupling/runners/engine-data-runner.js",
        "dependsOn": ["build"]
      }
    },
    "serve-engine-intelligence": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/runtime-decoupling/runners/engine-intelligence-runner.js",
        "dependsOn": ["build"]
      }
    },
    "serve-engine-optimization": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/runtime-decoupling/runners/engine-optimization-runner.js",
        "dependsOn": ["build"]
      }
    },
    "serve-engine-trust": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/runtime-decoupling/runners/engine-trust-runner.js",
        "dependsOn": ["build"]
      }
    },
    "serve-engine-api": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/runtime-decoupling/runners/engine-api-runner.js",
        "dependsOn": ["build"]
      }
    }
  }
}
