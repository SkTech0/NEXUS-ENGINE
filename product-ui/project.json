{
  "name": "product-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "product-ui/src",
  "tags": ["scope:ui", "type:app"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "product-ui",
        "command": "npx ng serve product-ui --configuration development --proxy-config proxy.conf.json"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "product-ui",
        "command": "npx ng build product-ui --configuration development"
      }
    }
  }
}
